<main>
  <h1><%= title %></h1>
<h3>Welcome New</h3>
  <% if (messages && messages.length > 0) { %>
    <div class="flash-message">
      <% messages.forEach(function(msg) { %>
        <p><%= msg %></p>
      <% }) %>
    </div>
  <% } %>
  <% if (errors) { %>
    <div class="errors">
      <% errors.forEach(function(error) { %>
        <p><%= error.msg %></p>
      <% }) %>
    </div>
  <% } %>
  <p>You're logged in.</p>
  <a href="/account/update">Edit Account Information</a>
</main>